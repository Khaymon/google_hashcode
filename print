#include <iostream>

using namespace std;

class Light {
  int time_;
  vector<pair<std::string, int>> streets_timing_;
};

void print(){
    // traffic_lights - ветктор Light-ов
    cout << traffic_lights.size(); << endl;
    for (int i = 0; i < traffic_lights; ++i){
        cout << traffic_lights[i].street_index_ << endl;
        for (const auto& elem : street_timing_){
            std::cout << elem.first << " " << elem.second << '\n';
        }
    }
}

